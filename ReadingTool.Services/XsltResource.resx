  &lt;/xsl:template&gt;

  &lt;xsl:template match="stats"&gt;
    &lt;div style="margin-top: 30px;"&gt;
      &lt;button class="btn" onclick="$('#stats').toggle();"&gt;
        Show some statistics for this text
