    <value>&lt;?xml version="1.0" encoding="utf-8"?&gt;
&lt;xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"&gt;

  &lt;xsl:template match="/root"&gt;
    &lt;xsl:apply-templates select="link"/&gt;
    &lt;xsl:apply-templates select="stats"/&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="link"&gt;
    &lt;xsl:apply-templates select="p[@side='first']"/&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="p[@side='first']"&gt;
    &lt;p&gt;
      &lt;xsl:attribute name="dir"&gt;
        &lt;xsl:value-of select="@dir" /&gt;
      &lt;/xsl:attribute&gt;
      
      &lt;xsl:apply-templates select="s"/&gt;
    &lt;/p&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="s"&gt;
    &lt;span class="sentence"&gt;
      &lt;xsl:apply-templates select="t"/&gt;
    &lt;/span&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="t"&gt;
    &lt;xsl:apply-templates select="multi"/&gt;
    &lt;xsl:choose&gt;
      &lt;xsl:when test="@type='term'"&gt;
        &lt;span&gt;
          &lt;xsl:attribute name="class"&gt;
            &lt;xsl:value-of select="@state" /&gt;
            &lt;xsl:text&gt; &lt;/xsl:text&gt;
            &lt;xsl:value-of select='@lower' /&gt;
            &lt;xsl:text&gt; &lt;/xsl:text&gt;
            &lt;xsl:value-of select='@box' /&gt;
          &lt;/xsl:attribute&gt;
          &lt;xsl:choose&gt;
            &lt;xsl:when test="@data"&gt;
              &lt;a&gt;
                &lt;xsl:attribute name="title"&gt;
                  &lt;xsl:value-of select="@data" /&gt;
                &lt;/xsl:attribute&gt;
                &lt;xsl:value-of select="@value" /&gt;
              &lt;/a&gt;
            &lt;/xsl:when&gt;
            &lt;xsl:otherwise&gt;
              &lt;xsl:value-of select="@value" /&gt;
            &lt;/xsl:otherwise&gt;
          &lt;/xsl:choose&gt;
        &lt;/span&gt;
      &lt;/xsl:when&gt;
      &lt;xsl:when test="@type='parallel'"&gt;
        &lt;xsl:value-of select="@value" /&gt;
      &lt;/xsl:when&gt;
      &lt;xsl:when test="@type='space'"&gt;
        &lt;xsl:choose&gt;
          &lt;xsl:when test="@inSpan='true'"&gt;
            &lt;span&gt;
              &lt;xsl:attribute name="class"&gt;wsx&lt;/xsl:attribute&gt;
              &lt;xsl:text&gt; &lt;/xsl:text&gt;
            &lt;/span&gt;
          &lt;/xsl:when&gt;
          &lt;xsl:otherwise&gt;
            &lt;xsl:text&gt; &lt;/xsl:text&gt;
          &lt;/xsl:otherwise&gt;
        &lt;/xsl:choose&gt;
      &lt;/xsl:when&gt;
      &lt;xsl:when test="@type='punctuation'"&gt;
        &lt;xsl:choose&gt;
          &lt;xsl:when test="@inSpan='true'"&gt;
            &lt;span&gt;
              &lt;xsl:attribute name="class"&gt;pcx&lt;/xsl:attribute&gt;
              &lt;xsl:value-of select="@value" /&gt;
            &lt;/span&gt;
          &lt;/xsl:when&gt;
          &lt;xsl:otherwise&gt;
            &lt;xsl:value-of select="@value" /&gt;
          &lt;/xsl:otherwise&gt;
        &lt;/xsl:choose&gt;
      &lt;/xsl:when&gt;
    &lt;/xsl:choose&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="multi"&gt;
    &lt;sup&gt;
      &lt;span&gt;
        &lt;xsl:attribute name="class"&gt;
          mxx
          &lt;xsl:text&gt; &lt;/xsl:text&gt;
          &lt;xsl:value-of select="@state" /&gt;
          &lt;xsl:text&gt; &lt;/xsl:text&gt;
          &lt;xsl:value-of select="@id" /&gt;
          &lt;xsl:text&gt; &lt;/xsl:text&gt;
          &lt;xsl:value-of select='@box' /&gt;
        &lt;/xsl:attribute&gt;
        &lt;xsl:attribute name="data-phrase"&gt;
          &lt;xsl:value-of select="." /&gt;
        &lt;/xsl:attribute&gt;
        &lt;xsl:attribute name="data-id"&gt;
          &lt;xsl:value-of select="@id" /&gt;
        &lt;/xsl:attribute&gt;
