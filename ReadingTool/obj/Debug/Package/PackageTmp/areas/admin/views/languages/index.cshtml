@model IEnumerable<SystemLanguageModel>
@{
    ViewBag.Title = "Index";
}
@section submenu { @Html.Partial("_menu") }

<table width="95%" cellpadding="0" cellspacing="0" border="0" class="texts" style="margin-left: 10px">
    <thead>
        <tr>
            <th>Language Name</th>
            <th>Language Code</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var l in Model)
        {
            <tr>
                <td>@l.Name</td>
                <td>@l.Code</td>
                <td>edit</td>
            </tr>
        }
    </tbody>
</table>