@model string
@{
    ViewBag.Title = "Import words from LWT";
}
@section submenu {
    @Html.Partial("_menu")
}
@section footer { }
<p class="warning">Some words were not imported. Please correct the errors below and try again.</p>

<table class="texts" border="0" cellpadding="0" cellspacing="0" style="margin-left: 10px" width="90%">
    <thead>
        <tr>
            <th>Errors</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var line in Model.Split(new[] { '\n'}, StringSplitOptions.RemoveEmptyEntries))
        {
            <tr>
                <td>@Html.Raw(line)</td>
            </tr>    
        }
    </tbody>
</table>
