@model ReadingTool.Models.Create.Import.ImportDataUploadModel
@{
    ViewBag.Title = "AddSetting";
}

@section submenu { @Html.Partial("_menu") }

@{ using (Html.BeginForm<SystemSettingsController>(x => x.Import(), FormMethod.Post, new { enctype = "multipart/form-data" }))
   {
        @Html.ValidationSummary(true)
        @Html.AntiForgeryToken()
        <legend>Load Setting</legend>
    
       <div class="input_wrapper">  
           @Html.LabelHelpFor(x=>x.File)
           <input type="file" name="File"/>
            @Html.ValidationMessageFor(x => x.File)
       </div>
        <button type="submit">
            <span>import settings</span>
        </button>
   }
}
